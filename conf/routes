# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET    /health                                     @controllers.health.HealthCheckController.checkSystemHealth()
POST   /v3/dialcode/generate                       @controllers.dialcode.DialcodeV3Controller.generateDialCode()
GET    /v3/dialcode/read/:id                       @controllers.dialcode.DialcodeV3Controller.readDialCode(id: String)
PATCH  /v3/dialcode/update/:id                     @controllers.dialcode.DialcodeV3Controller.updateDialCode(id: String)
POST   /v3/dialcode/list                           @controllers.dialcode.DialcodeV3Controller.listDialCode()
POST   /v3/dialcode/search                         @controllers.dialcode.DialcodeV3Controller.searchDialCode()
POST   /v3/dialcode/sync                           @controllers.dialcode.DialcodeV3Controller.syncDialCode()
POST   /v3/dialcode/publish/:id                    @controllers.dialcode.DialcodeV3Controller.publishDialCode(id: String)
POST   /v3/dialcode/publisher/create               @controllers.dialcode.DialcodeV3Controller.createPublisher()
GET    /v3/dialcode/publisher/read/:id             @controllers.dialcode.DialcodeV3Controller.readPublisher(id: String)
PATCH  /v3/dialcode/publisher/update/:id           @controllers.dialcode.DialcodeV3Controller.updatePublisher(id: String)

POST   /v3/dialcode/period/read                   @controllers.StaffRoomController.getPeriodDetails()
GET   /v3/dialcode/schedule/read/:date/:id        @controllers.StaffRoomController.getTimeTable(date: String, id: String)
GET   /v3/dialcode/question/read/:topic           @controllers.StaffRoomController.getQuestions(topic: String)
